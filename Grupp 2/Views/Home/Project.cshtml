@model IEnumerable<Data.Models.Project>

@{
    ViewBag.Title = "Projekt";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<br />
<br />

@using (Html.BeginForm("Search", "Home", FormMethod.Get))
{

}

<table class="table table-striped">
    <thead>
        <tr>
            <th>Titel</th>
            <th>Beskrivning</th>
        </tr>

    </thead>
    <tbody>
        @if (Model.Count() == 0)
        {
            <tr>
                <td colspan="3" style="color:red"> Inga projekt är för tillfället inlagda i systemet!</td>

            </tr>
        }
        else
        {
            foreach (var item in Model)
            {
                <tr>
                    <td>@item.Titel</td>
                    <td>@item.Description</td>
                </tr>
            }
        }
    </tbody>


</table>